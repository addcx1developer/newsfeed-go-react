    "watch-relay": "relay-compiler --watch",
    "watch-server": "air",
    "watch-vite": "vite",
    "dev": "concurrently npm:watch-* -c auto"
  },
  "dependencies": {
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-relay": "^20.1.1",
    "relay-runtime": "^20.1.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/node": "^24.6.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@types/react-relay": "^18.2.1",
    "@types/relay-runtime": "^19.0.3",
    "@vitejs/plugin-react": "^5.0.4",
    "babel-plugin-relay": "^20.1.1",
    "concurrently": "^9.2.1",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "graphql": "^16.11.0",
    "husky": "^9.1.7",
    "prettier": "^3.6.2",
    "relay-compiler": "^20.1.1",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.45.0",
    "vite": "^7.1.7"
  }
